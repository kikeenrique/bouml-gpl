class UmlAcceptEventAction
!!!204546.cpp!!!	write(inout out : FileOut) : void
  write_begin(out, "AcceptEventAction");
  if (isUnmarshall())
    out << " isUnmarshall=\"true\"";
  
  QCString trig;
  
  switch (_lang) {
  case Uml:
    trig = trigger();
    break;
  case Cpp:
    trig = cppTrigger();
    break;
  default:
    // java
    trig = javaTrigger();
  }

  if (! trig.isEmpty()) {
    out << " trigger=\"";
    out .quote(trig);
    out << '"';
  }

  write_end(out);
