class UmlBroadcastSignalAction
!!!204162.cpp!!!	write(inout out : FileOut) : void
  write_begin(out, "BroadcastSignalAction");
  
  QCString sig;
  
  switch (_lang) {
  case Uml:
    sig = signal();
    break;
  case Cpp:
    sig = cppSignal();
    break;
  default:
    // java
    sig = javaSignal();
  }
  if (! sig.isEmpty()) {
    out << " signal=\"";
    out.quote(sig);
    out << '"';
  }

  write_end(out);
